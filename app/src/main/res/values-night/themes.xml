<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SeekBar" parent="Base.Widget.AppCompat.SeekBar.Discrete">
        <item name="android:progressBackgroundTint">@color/primary1</item>
        <item name="android:progressTint">@color/primary2</item>
        <item name="android:colorControlActivated">@color/primary1</item>
        <item name="android:thumbTint">@color/primary2</item>
        <item name="tickMark">@drawable/dark_tick_mark</item>
